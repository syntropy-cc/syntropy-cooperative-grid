{
  "minimal": {
    "owner_name": "Test User",
    "owner_email": "test@example.com",
    "network_config": null,
    "security_config": null,
    "custom_settings": {}
  },
  "complete": {
    "owner_name": "Complete User",
    "owner_email": "complete@example.com",
    "network_config": {
      "discovery": {
        "enabled": true,
        "port": 8080
      },
      "api": {
        "enabled": true,
        "port": 9090,
        "tls": {
          "enabled": true,
          "cert_path": "/path/to/cert.pem",
          "key_path": "/path/to/key.pem"
        }
      }
    },
    "security_config": {
      "encryption": {
        "algorithm": "ed25519",
        "key_rotation": {
          "enabled": true,
          "interval": "30d"
        }
      },
      "backup": {
        "enabled": true,
        "interval": "24h",
        "retention": "30d"
      }
    },
    "custom_settings": {
      "log_level": "info",
      "debug_mode": "false"
    }
  },
  "network_only": {
    "owner_name": "Network User",
    "owner_email": "network@example.com",
    "network_config": {
      "discovery": {
        "enabled": true,
        "port": 8080
      },
      "api": {
        "enabled": true,
        "port": 9090,
        "tls": {
          "enabled": false,
          "cert_path": "",
          "key_path": ""
        }
      }
    },
    "security_config": null,
    "custom_settings": {}
  },
  "security_only": {
    "owner_name": "Security User",
    "owner_email": "security@example.com",
    "network_config": null,
    "security_config": {
      "encryption": {
        "algorithm": "ed25519",
        "key_rotation": {
          "enabled": true,
          "interval": "7d"
        }
      },
      "backup": {
        "enabled": true,
        "interval": "12h",
        "retention": "7d"
      }
    },
    "custom_settings": {}
  }
}
